{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Bugs Submitted",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(triangulum_bugs_submitted_total)",
          "legendFormat": "Total"
        }
      ]
    },
    {
      "id": 2,
      "title": "Bugs Fixed (Success)",
      "type": "stat",
       "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(triangulum_bugs_fixed_success_total)",
          "legendFormat": "Success"
        }
      ]
    },
    {
      "id": 3,
      "title": "Bugs Fixed (Failure)",
      "type": "stat",
       "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(triangulum_bugs_fixed_failure_total)",
          "legendFormat": "Failure"
        }
      ]
    },
    {
      "id": 4,
      "title": "Active vs Queued Sessions",
      "type": "timeseries",
       "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "triangulum_active_sessions",
          "legendFormat": "Active"
        },
        {
          "expr": "triangulum_queued_tickets",
          "legendFormat": "Queued"
        }
      ]
    },
    {
      "id": 5,
      "title": "Bug Fix Duration (Histogram)",
      "type": "histogram",
       "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(triangulum_bug_fix_duration_seconds_sum[5m])) by (le) / sum(rate(triangulum_bug_fix_duration_seconds_count[5m])) by (le)"
        }
      ]
    }
  ],
  "schemaVersion": 37,
  "style": "dark",
  "tags": [],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Triangulum System Dashboard",
  "uid": "triangulum-dashboard",
  "version": 1,
  "weekStart": ""
}
